[expected-request]
GET PATH HTTP/1.1
Accept: */*
Accept-Encoding: deflate, gzip, br
Content-Type: text/plain; charset=utf-8
Cookie: yummy_cookie=choco; tasty_cookie=strawberry
Host: HOST

[response]
HTTP/1.1 302 Found
Date: xxx, xx xxx xxxx xx:xx:xx xxx
Location: /response-redirect-dest-with-cookies



[test]
// FSHARPONLY
(let reqHeaders = { Cookie = "yummy_cookie=choco; tasty_cookie=strawberry" }
 let response = HttpClient.get_v5_ster "http://URL" {} reqHeaders in
 let respHeaders = response.headers |> Dict.remove_v0 "Date" in
 Dict.set_v0 response "headers" respHeaders) =
   { body = "Redirect destination reached with cookies"
     code = 200
     error = ""
     headers =
      {
        ``Arbitrary-header`` = "Test value"
        ``Content-Length`` = "41"
        ``Content-Type`` = "text/plain; charset=utf-8"
        ``HTTP/1.1 200 OK`` = ""
        Server = "Kestrel"
      }
     raw = "Redirect destination reached with cookies"}

