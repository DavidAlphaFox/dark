[expected-request]
GET PATH HTTP/1.1
Accept: */*
Accept-Encoding: deflate, gzip, br
Content-Type: text/plain; charset=utf-8
Host: HOST

[response]
HTTP/1.1 302 Found
Redirect-header-2: 2
Redirect-header: 2
Location: /response-redirect-chain-2-ocaml
Date: xxx, xx xxx xxxx xx:xx:xx xxx


[test]
// FSHARPONLY
(let response = HttpClient.get_v5_ster "http://URL" {} {}
 let respHeaders = response.headers |> Dict.remove_v0 "Date"
 Dict.set_v0 response "headers" respHeaders) =
  { body = "Redirect destination reached"
    code = 200
    error = ""
    headers =
      {
        ``Arbitrary-header`` = "Test value"
        ``Content-Length`` = "28"
        ``Content-Type`` = "text/plain; charset=utf-8"
        ``HTTP/1.1 200 OK`` = ""
        Server = "Kestrel"
      }
    raw = "Redirect destination reached"
  }
