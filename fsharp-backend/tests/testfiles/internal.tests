[tests.grants]

[test.empty grants]
(let _ = DarkInternal.insertUser_v1_ster "empty_grants" "a@eg.com" "test user"
 DarkInternal.orgsFor "empty_grants") = {}

[test.grants and orgs]
(let _ = DarkInternal.insertUser_v1_ster "g_and_o_org" "org@gaa.com" "test user"
 let _ = DarkInternal.insertUser_v1_ster "g_and_o_user" "user@gaa.com" "test user"
 let _ = DarkInternal.grant "g-a-o-user" "g_and_o_org" "rw" in
 DarkInternal.orgsFor "g_a_o_user") = { ``g_and_o_org`` = "rw" }

[test.grants and grants]
(let _ = DarkInternal.grant "test" "dark" "rw" in
 let result = DarkInternal.grantsFor "dark"
 let _ = DarkInternal.grant "test" "dark" "" in
 result) = { test = "rw" }

