#!/usr/bin/env bash

set -euo pipefail

# Clear all the volumes that we use

# Must be run from outside (stopped) container

docker volume rm \
  dark_build \
  dark_dotesy \
  dark_esy \
  dark_fsharp_build \
  dark_lib \
  dark_node_modules \
  dark_queue_scheduler_target \
  dark_rust_cargo \
  dark_stroller_target \
  dark_nuget \
  darklang-dark-extension-volume \
  darklang-dark-extension-volume-insiders \
  vscode