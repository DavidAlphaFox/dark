[<HttpHandler("GET", "/")>]
let _indexHandler _req =
  let files = Directory.pwd |> Directory.ls |> Builtin.Json.serialize<List<String>>

  let respBody =
    match files with
    | Ok files -> Builtin.String.toBytes files
    | Error _ -> "failed :("

  PACKAGE.Darklang.Stdlib.Http.response respBody 200