// End-user password hashing and checking works
PACKAGE.Darklang.Stdlib.Password.check_v0
  (PACKAGE.Darklang.Stdlib.Password.hash_v0 "password")
  "password" = true

PACKAGE.Darklang.Stdlib.Password.check_v0
  (PACKAGE.Darklang.Stdlib.Password.hash_v0 "👱👱🏻👱🏼👱🏽👱🏾👱")
  "👱👱🏻👱🏼👱🏽👱🏾👱" = true

PACKAGE.Darklang.Stdlib.Password.check_v0
  (PACKAGE.Darklang.Stdlib.Password.hash_v0 "")
  "" = true

// Password hashes do not equal each other
PACKAGE.Darklang.Stdlib.Password.hash_v0 "my password"
== PACKAGE.Darklang.Stdlib.Password.hash_v0 "my password" = false

PACKAGE.Darklang.Stdlib.Password.hash_v0 ""
== PACKAGE.Darklang.Stdlib.Password.hash_v0 "" = false

PACKAGE.Darklang.Stdlib.Password.hash_v0 "my password"
== PACKAGE.Darklang.Stdlib.Password.hash_v0 "not my password" = false