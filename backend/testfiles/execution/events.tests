type FruitRecord = {
  fruits : List<String>
}

// getQueue works
Test.getQueue_v0 "TestWorker" = []

// emit works
(let _ = emit "value" "TestWorker" in
 let queue = Test.getQueue_v0 "TestWorker" in
 queue) = ["\"value\""]

// emit works with mixed values
(let _ = emit "value" "TestWorker" in
 let _ = emit 1 "TestWorker" in
 let _ = emit (FruitRecord {fruits = ["apple", "banana"] }) "TestWorker" in
 let queue = Test.getQueue_v0 "TestWorker" in
 List.sort queue) =
   [
    "\"value\"";
    "1";
    "FruitRecord {\n  fruits: [\n    \"apple\", \"banana\"\n  ]\n}"]