module Count =
  // empty
  DB.count X = 0

  // one entries
  (let _ = DB.set_v1 { x = "hello" } "one" X in
   DB.count X) = 1

  // multiple entries
  (let _ = DB.set_v1 { x = "hello" } "one" X in
   let _ = DB.set_v1 { x = "goodbye" } "two" X in
   let _ = DB.set_v1 { x = "howdy" } "three" X in
   let _ = DB.set_v1 { x = "cheers" } "four" X in
   DB.count X) = 4

